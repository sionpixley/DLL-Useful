<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <Authors>Sion Pixley</Authors>
    <Company>PTC</Company>
    <PackageLicenseFile></PackageLicenseFile>
    <RepositoryType>git</RepositoryType>
    <RepositoryUrl>https://github.com/sionpixley/DLL-Useful/tree/main</RepositoryUrl>
    <NeutralLanguage>en-US</NeutralLanguage>
    <PackageReleaseNotes>Fixing some holes in the DFS algo.</PackageReleaseNotes>
    <Description>NuGet package of useful methods.</Description>
    <Version>2.2.1</Version>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <AssemblyName>Sion.Useful</AssemblyName>
    <RootNamespace>Sion.Useful</RootNamespace>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <PackageTags>sion;useful;string methods;datetime methods;decimal methods;graph;directed graph;weighted graph;directed weighted graph</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\README.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

</Project>
